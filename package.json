{
  "name": "koa-session2",
  "version": "1.0.10",
  "description": "Middleware for Koa2 to get/set session use with custom stores such as Redis or mongodb with Babel",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "test": "mocha --compilers js:@babel/register --require @babel/core --require @babel/polyfill",
    "test-cov": "node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --compilers js:@babel/register --require @babel/core --require @babel/polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Secbone/koa-session2.git"
  },
  "keywords": [
    "koa2",
    "koa",
    "session",
    "redis",
    "store",
    "middleware"
  ],
  "author": "Secbone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Secbone/koa-session2/issues"
  },
  "homepage": "https://github.com/Secbone/koa-session2#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "koa": "^2.0.0",
    "koa-router": "^7.0.1",
    "mocha": "^2.5.1",
    "supertest": "^1.2.0"
  },
  "publishConfig": {
    "tag": "babel"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
